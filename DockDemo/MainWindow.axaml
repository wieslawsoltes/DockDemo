            <TextBlock Text="Tab2"/>
        </TabItem>
        <TabItem Header="Tab3">
            <TextBlock Text="Tab3"/>
        </TabItem>
    </TabControl>-->
    <ListBox>
        <ListBox.ItemsPanel>
            <ItemsPanelTemplate>
                <StackPanel Orientation="Horizontal"/>
            </ItemsPanelTemplate>
        </ListBox.ItemsPanel>
        <ListBoxItem>
            <TextBlock Text="Item1"/>
        </ListBoxItem>
        <ListBoxItem>
            <TextBlock Text="Item2"/>
        </ListBoxItem>
        <ListBoxItem>
            <TextBlock Text="Item3"/>
        </ListBoxItem>
    </ListBox>
</Window>
